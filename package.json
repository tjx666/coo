    "scripts": {
        "start": "concurrently -r -i --kill-others-on-fail yarn:clean yarn:start-renderer yarn:start-main",
        "start-main": "wait-on http-get://localhost:3000/ && nodemon",
        "start-renderer": "cross-env-shell NODE_ENV=development ts-node --files -P scripts/tsconfig.json scripts/start",
        "dev-analyze": "yarn run start-renderer --analyze",
        "build": "cross-env-shell NODE_ENV=production ts-node --files -P scripts/tsconfig.json scripts/build",
        "build-dll": "yarn run build --dll",
        "build-analyze": "yarn run build --analyze",
        "test": "echo 'skip test...'",
        "lint": "yarn run lint-eslint && yarn run lint-stylelint",
        "lint-eslint": "eslint -c .eslintrc.js --ext .ts,.tsx,.js {src,scripts}/**/*.{ts,tsx,js}",
        "lint-stylelint": "stylelint --config .stylelintrc.json src/renderer/**/*.scss --syntax scss",
        "clean": "rimraf -rf out dist",
        "upgrade": "yarn upgrade --latest && yarn run build-dll",
        "reinstall": "rimraf -rf node_modules && yarn && yarn run build-dll"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,tsx,js}": [
            "eslint -c .eslintrc.js"
        ],
        "*.scss": [
            "stylelint --config .stylelintrc.json"
        ],
        "*.{ts,tsx,js,json,scss,md}": [
            "prettier --write"
        ]
    },
