    },
    "repository": {
        "url": "git@github.com:tjx666/coo.git",
        "type": "git"
    },
    "scripts": {
        "start": "concurrently -r -i --kill-others-on-fail yarn:clean yarn:start-renderer yarn:start-main",
        "start-main": "wait-on http-get://localhost:3000/ && nodemon",
        "start-renderer": "cross-env NODE_ENV=development ts-node --files -P scripts/tsconfig.json scripts/start",
        "build": "cross-env NODE_ENV=production ts-node --files -P scripts/tsconfig.json scripts/build",
        "build-analyze": "cross-env NODE_ENV=production ts-node --files -P scripts/tsconfig.json scripts/build --analyze",
        "test": "yarn run lint",
        "lint": "yarn run lint-eslint && yarn run lint-stylelint",
        "lint-eslint": "eslint -c .eslintrc.js --ext .ts,.tsx,.js {src,scripts}/**/*.{ts,tsx,js}",
        "lint-stylelint": "stylelint --config .stylelintrc.json src/renderer/**/*.scss --syntax scss",
        "clean": "rimraf -rf out dist",
        "clean-hard-source": "rimraf ./node_modules/.cache/hard-source",
        "reinstall": "rimraf -rf node_modules && yarn"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,tsx,js}": [
            "eslint -c .eslintrc.js"
        ],
        "*.scss": [
            "stylelint --config .stylelintrc.json"
        ],
        "*.{ts,tsx,js,json,scss,md}": [
            "prettier --write"
        ]
    },
