{
    "path-intellisense.mappings": {
        "assets": "${workspaceRoot}/src/renderer/assets",
        "styles": "${workspaceRoot}/src/renderer/styles",
        "lib": "${workspaceRoot}/src/renderer/lib",
        "utils": "${workspaceRoot}/src/renderer/utils",
        "pages": "${workspaceRoot}/src/renderer/pages",
        "layouts": "${workspaceRoot}/src/renderer/layouts",
        "components": "${workspaceRoot}/src/renderer/components"
    },
    "search.exclude": {
        "**/node_modules": true,
        "out": true,
        "dist": true,
        "yarn.lock": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "out": true,
        "dist": true,
        "yarn.lock": true
    }
}
